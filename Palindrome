
public class Palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	       String a="aba";
	         String reversed="";
	         int b=a.length()-1;
	         while(b>=0) {
	        	 reversed = reversed + a.charAt(b);
	        	 b--;
	         }
	         System.out.println(a.equals(reversed));	        

	}

}
